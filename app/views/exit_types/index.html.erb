<p id="notice"><%= notice %></p>

<h1>Exit Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Disabled</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exit_types.each do |exit_type| %>
      <tr>
        <td><%= exit_type.name %></td>
        <td><%= exit_type.disabled %></td>
        <td><%= exit_type.organization_id %></td>
        <td><%= link_to 'Show', exit_type %></td>
        <td><%= link_to 'Edit', edit_exit_type_path(exit_type) %></td>
        <td><%= link_to 'Destroy', exit_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exit Type', new_exit_type_path %>
