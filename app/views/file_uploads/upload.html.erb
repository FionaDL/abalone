<section class="px-6 py-12">
  <div class="container">
    <h1 class="text-title3 mb-4">File Upload(s)</h1>
    <p class="mb-4">
      <%= link_to "Upload More Files", new_file_upload_path,
          class: "bg-blue-600 border border-transparent cursor-pointer px-4 py-1 rounded text-white" %>
      <%= link_to "View All Files", file_uploads_path,
          class: "bg-blue-600 border border-transparent cursor-pointer px-4 py-1 rounded text-white" %>
    </p>
    <% @file_uploads.each do |file_upload| %>
      <p>
        <b><%= file_upload.result_message %></b><br />
        Category: <%= file_upload.category %><br />
        Filename: <%= file_upload.filename %><br />
      </p>
    <% end %>
  </div>
</section>
